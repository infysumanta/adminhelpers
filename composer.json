{
  "name": "sumantablog/adminhelpers",
  "description": "Helpers extension for laravel-superadmin",
  "type": "library",
  "keywords": ["laravel-superadmin", "helpers"],
  "homepage": "https://github.com/sumantablog/adminhelpers",
  "license": "MIT",
  "authors": [{
    "name": "Sumanta Kabiraj",
    "email": "sumantablog@gmail.com"
  }],
  "require": {
    "php": ">=7.0.0",
    "sumantablog/laravel-superadmin": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0"
  },
  "autoload": {
    "psr-4": {
      "Sumantablog\\Admin\\Helpers\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Sumantablog\\Admin\\Helpers\\HelpersServiceProvider"
      ]
    }
  }
}